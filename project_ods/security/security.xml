<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="group_project_ods_fields_visible" model="res.groups">
        <field name="name">Projects with IHO Service order fields access</field>
        <field name="category_id" ref="base.module_category_project"/>
        <field name="users" eval="[
            (4, ref('base.user_admin')),
            (4, ref('base.user_root')),
        ]"/>
        <field name="comment">Allows to see project added fields for IHO Service orders</field>
    </record>
    <record id="group_project_ods_all_access" model="res.groups">
        <field name="name">Projects with IHO Service order all access</field>
        <field name="category_id" ref="base.module_category_project"/>
        <field name="users" eval="[
            (4, ref('base.user_admin')),
            (4, ref('base.user_root')),
        ]"/>
        <field name="comment">Allows to manage config for IHO Service orders</field>
    </record>
</odoo>
